<section id="hero" class="c-hero">
  <div class="c-hero__title">
    <h1>
      <span class="u-text-semibold">@model.hero.heading</span><br />
      @model.hero.subHeading
    </h1>
  </div>
  <div class="c-hero__text">
    @model.hero.text.forEach(function (paragraph) {
      <p>@paragraph</p>
    })
  </div>
</section>